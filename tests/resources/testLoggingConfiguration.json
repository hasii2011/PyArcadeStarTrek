{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "starTrekSimple": {
            "format": "%(asctime)s.%(msecs)06d - %(levelname)s: %(module)s: %(message)s",
            "datefmt" : "%H:%M:%S"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "formatter": "starTrekSimple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "ERROR",
            "handlers": ["consoleHandler"],
            "propagate": "False"
        },
        "tests": {
            "level": "WARNING",
            "propagate": "False"
        },
        "__main__": {
            "level": "WARNING",
            "propagate": "False"
        },
        "Test Shooter": {
            "level": "INFO",
            "propagate": "False"
        },
        "TestSmoothMotion": {
            "level": "WARNING",
            "propagate": "False"
        },
        "TestEventEngineScheduler": {
            "level": "INFO",
            "propagate": "False"
        },
        "tests.pytrek.model.TestQuadrant": {
            "level": "WARNING",
            "propagate": "False"
        },
        "tests.pytrek.engine.TestIntelligence": {
            "level": "WARNING",
            "propagate": "False"
        },
        "tests.pytrek.engine.TestGameEngine": {
            "level": "WARNING",
            "propagate": "False"
        },
        "tests.pytrek.engine.TestDevices": {
            "level": "WARNING",
            "propagate": "False"
        },
        "tests.pytrek.engine.TestComputer": {
            "level": "WARNING",
            "propagate": "False"
        },
        "tests.pytrek.gui.TestGamePiece": {
            "level": "WARNING",
            "propagate": "False"
        },
        "pytrek.engine.GameEngine": {
            "level": "WARNING",
            "propagate": "False"
        },
        "pytrek.engine.Intelligence": {
            "level": "WARNING",
            "propagate": "False"
        },
        "pytrek.engine.futures.EventEngine": {
            "level": "INFO",
            "propagate": "False"
        },
        "pytrek.engine.futures.FutureEventHandlers": {
            "level": "INFO",
            "propagate": "False"
        },

        "pytrek.model.Quadrant": {
            "level": "WARNING",
            "propagate": "False"
        },
        "pytrek.model.Galaxy": {
            "level": "INFO",
            "propagate": "False"
        },
        "pytrek.gui.gamepieces.SmoothMotion": {
            "level": "WARNING",
            "propagate": "False"
        },
        "pytrek.gui.CourseDistanceDialog": {
            "level":     "INFO",
            "propagate": "False"
        },
        "pytrek.mediators.EnterprisePhaserMediator": {
            "level":     "WARNING",
            "propagate": "False"
        }
    }
}

{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "starTrekSimple": {
            "format": "%(asctime)s.%(msecs)06d - %(levelname)s: %(module)s: %(message)s",
            "datefmt" : "%H:%M:%S"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "formatter": "starTrekSimple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "ERROR",
            "handlers": ["consoleHandler"],
            "propagate": "False"
        },
        "tests": {
            "level": "WARNING",
            "propagate": "False"
        },
        "TestIntelligence": {
            "level": "INFO",
            "propagate": "False"
        },
        "tests.TestComputer": {
            "level": "INFO",
            "propagate": "False"
        },
        "TestQuadrant": {
            "level": "INFO",
            "propagate": "False"
        },
        "TestSmoothMotion": {
            "level": "INFO",
            "propagate": "False"
        },
        "pytrek.engine.Intelligence": {
            "level": "INFO",
            "propagate": "False"
        },
        "pytrek.model.Quadrant": {
            "level": "INFO",
            "propagate": "False"
        },
        "pytrek.model.Galaxy": {
            "level": "DEBUG",
            "propagate": "False"
        },
        "pytrek.gui.gamepieces.SmoothMotion": {
            "level": "INFO",
            "propagate": "False"
        }

    }
}
